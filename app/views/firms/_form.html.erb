<%= simple_form_for @firm do |f| %>
  <%= f.input :name %>
  <%= f.input :logo %>
  <%= f.input :datestamp %>
  <%= f.input :people %>
  <div class="field">
    <%= f.collection_check_boxes :skill_ids, Skill.all, :id, :name do |b| %>
      <div class="collection-check-box">
          <%= b.check_box %>
          <%= b.label %>
      </div>
    <% end %>
  </div>
  <%= f.button :submit %>
<% end %>
