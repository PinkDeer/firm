<%= simple_form_for @firm do |f| %>
  <%= f.input :name %>
  <%= f.input :logo %>
  <%= f.input :datestamp %>
  <%= f.input :people %>
  <%= f.collection_check_boxes :skill_ids, Skill.all, :id, :name do |b| %>
    <div class="collection-check-box">
      <%= b.check_box %>
      <%= b.label %>
    </div>
  <% end %>
  <br>
  <%= f.button :submit %>
<% end %>
